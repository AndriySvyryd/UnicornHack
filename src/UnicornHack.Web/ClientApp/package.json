{
  "name": "unicornhack.web",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@microsoft/signalr": "3.1.0",
    "@microsoft/signalr-protocol-msgpack": "3.1.0",
    "@types/history": "^4.7.3",
    "@types/lodash-es": "^4.17.3",
    "@types/msgpack5": "^3.4.1",
    "@types/node": "^12.12.8",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-overlays": "^1.1.3",
    "@types/react-transition-group": "^4.2.3",
    "@types/webpack": "^4.41.0",
    "@types/webpack-env": "^1.14.1",
    "aspnet-webpack": "^3.0.0",
    "aspnet-webpack-react": "^4.0.0",
    "bootstrap": "^4.3.1",
    "buffer": "^5.4.3",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "css-loader": "^3.2.0",
    "css-modules-typescript-loader": "^4.0.0",
    "domhandler": "^3.0.0",
    "exports-loader": "^0.7.0",
    "file-loader": "^4.2.0",
    "fs-extra": "^8.1.0",
    "htmlparser2": "^4.0.0",
    "imports-loader": "^0.8.0",
    "is-wsl": "^2.1.1",
    "jest": "^25.0.0",
    "jest-environment-jsdom-fifteen": "^1.0.0",
    "jest-resolve": "^25.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "json-loader": "^0.5.7",
    "lodash-es": "^4.17.15",
    "merge": "^1.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "mobx": "^5.15.0",
    "mobx-react": "^6.1.4",
    "node-sass": "^4.13.1",
    "node-sass-utils": "^1.1.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.12.0",
    "react-dev-utils": "9.1.0",
    "react-dom": "^16.12.0",
    "react-hotkeys": "^2.0.0",
    "react-overlays": "^2.1.0",
    "react-transition-group": "^4.3.0",
    "resolve": "^1.12.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.3.4",
    "ts-loader": "^6.2.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "scripts": {
    "start": "rimraf ./build && node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "cross-env CI=true node scripts/test.js --env=jest-environment-jsdom-fifteen"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "scripts/jsdom.js"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fifteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "browserslist": [
    "last 2 versions and last 2 years and >1%",
    "not dead"
  ]
}
